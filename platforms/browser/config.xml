<?xml version='1.0' encoding='utf-8'?>
<widget id="com.GuiGhostGames.StarMatch2048" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>2048 StarMatch</name>
    <description>
        Swipe up down left or right to merge the stars. Can you get to the elusive Rainbow star?
    </description>
    <author email="info@guighost.com" href="http://guighostgames.com">
		GuiGhost Games
    </author>
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <content src="index.html" />
    <access origin="*" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
